<div class="card text-left mx-auto shadow-lg rounded" style="max-width: 80%; margin-top: 50px">
        <div class="card-header text-white text-center">
          New Registration
        </div>
        <%- include('./partials/messages'); %>
        <form action="/register" method="POST">
        <div class="card-body">
            
                <div class="mx-auto" style="width: 80%">
                    <div class="form-group">
                    <label for="GSTN">GSTN</label>
                    <input type="text" class="form-control" id="GSTN" aria-describedby="emailHelp" name="GSTN" placeholder="Enter GSTN">
                    </div>
                    <div class="form-group">
                        <label for="passwordInput">Create Password</label>
                        <input type="password" class="form-control" id="passwordInput" name="password" aria-describedby="emailHelp" placeholder="Enter Password">
                        </div>
                        <div class="form-group">
                            <label for="confirmPassword">Confirm Password</label>
                            <input type="password" class="form-control" id="confirmPassword" name="confirmPassword" aria-describedby="emailHelp" placeholder="Confirm Password">
                            </div>
                        <div class="form-group">
                        <label for="businessName">Legal Name of the Business (As Mentioned in PAN)</label>
                        <input type="text" class="form-control" id="businessName" name="businessName" aria-describedby="emailHelp" placeholder="Enter Legal Name of Business">
                        </div>
                    <div class="form-group">
                    <label for="pan">Permanent Account Number (PAN)</label>
                    <input type="text" class="form-control" id="pan" name="PAN" placeholder="Enter Permanent Account Number (PAN)">
                    </div>
                    <div class="form-group">
                            <label for="state">State/UT</label>
                            <select class="form-control" id="state" name="StateName"> 
                              <option selected>Select</option>
                              <option>Uttar Pradesh</option>
                            </select>
                    </div>
                    <div class="form-group">
                            <label for="district">District</label>
                            <select class="form-control" id="district" name="DistrictName">
                                    <option selected>Select</option>
                                <option>Ghaziabad</option>
                                <option>Kanpur</option>
                                <option>Lucknow</option>
                                <option>Meerut</option>
                                <option>Prayagraj</option>
                            </select>
                    </div>
                    <div class="form-group">
                        <label for="emailId">Email Address</label>
                        <div class="input-group">
                        <div class="input-group-prepend">
                        <span class="input-group-text"><i class="fas fa-envelope"></i></span>
                        </div>    
                            <input id="emailId" type="text" class="form-control" name="email" placeholder="Email">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="mobile">Mobile Number</label>
                        <div class="input-group">
                        <div class="input-group-prepend">
                        <span class="input-group-text"><i class="fas fa-mobile"></i></span>
                        </div>    
                            <input id="mobile" type="number" class="form-control" name="mobileNo" placeholder="Enter Mobile Number" maxlength="10">

                        </div>
                            </div>
                    </div>  
                </div>
                <div class="card-footer text-white text-center">
                    <button type="submit" class="btn btn-dark">PROCEED</button>
                  </div>
                    </form>
            
